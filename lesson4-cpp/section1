struct Seqlist
{
    int* a;
    int size;
    int capacity;

};
int& getvalue(Seqlist* p,int pos)
{
    return p->a[pos];
}
Seqlist SLInit()
{
    p->a=(int*)malloc(sizeof(int)*10);
    p->size=0;
    p->capacity=10;
    return p;
}
int main()
{
    Seqlist p=SLInit();
    int x=getvalue(1);


    return 0;
}
